{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "run-p build:watch start:dev",
    "build": "run-s typescript babel",
    "build:watch": "run-p typescript:watch babel:watch",
    "start": "node dist/index.js",
    "start:dev": "nodemon dist/index.js",
    "typescript": "tsc -p tsconfig.json",
    "typescript:watch": "tsc -p tsconfig.json -w",
    "babel": "babel build -d dist",
    "babel:watch": "babel build -d dist -w"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.4",
    "fastify-plugin": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@types/node": "^17.0.23",
    "nodemon": "^2.0.15",
    "prisma": "^3.12.0",
    "ts-node": "^10.7.0",
    "tsconfig": "*",
    "typescript": "^4.6.3",
    "yarn-run-all": "^3.1.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
